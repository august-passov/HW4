<h1>Places</h1>

<ul>
  <% for place in @places %>
    <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
  <% end %>
</ul>

<div class="d-grid gap-2 d-md-block position-relative m-12", class="position-absolute top-0 end-0">
    <a href="/places/new" class="btn btn-primary btn-lg">New Place</a>
  </div>
</div>